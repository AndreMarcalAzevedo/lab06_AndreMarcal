package gameCenter;

public class Jogo {
	private String nome;
	private double preco;
	private int highscore = 0;
	private int jogadas = 0;
	private int zeradas = 0;
	private String tipo;
	
	public void registraJogada(int score, boolean zerou){
		if (score > highscore) {
			highscore = score;
		}
	}
}